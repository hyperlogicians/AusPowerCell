{
  "expo": {
    "name": "AusPowerCell",
    "slug": "auspowercell-valves-control",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.auspowercell.valvescontrol"
    },
    "android": {
      "package": "com.auspowercell.valvescontrol"
    },
    "web": {
      "bundler": "metro",
      "head": {
        "link": [
          {
            "rel": "stylesheet",
            "href": "https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&display=swap"
          }
        ]
      }
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "auspowercell"
  }
}
